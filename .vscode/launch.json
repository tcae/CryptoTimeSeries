{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "julia",
            "request": "launch",
            "name": "Launch assetsdownload",
            "program": "${workspaceFolder}/scripts/assetsdownload.jl",
            "stopOnEntry": false
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "Launch setup",
            "program": "${workspaceFolder}/scripts/setup.jl",
            "stopOnEntry": false
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "Launch cryptocockpit",
            "program": "${workspaceFolder}/scripts/cryptocockpit.jl",
            "stopOnEntry": false
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "Run active Julia file",
            "program": "${file}",
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "juliaEnv": "${command:activeJuliaEnvironment}"
        }
    ]
}